
public class forLoop {
	public static void main(String[] args) {
//		for (int x = 1; x <= 5; x++) {
//			System.out.print(x);
//			for (int y = x-1; y > 0; y--) {
//			System.out.print(" "+y);
//			}
//		System.out.println();
		factorial(3);
		}
		        	
	private static long factorial(int n)
	{
	    if (n == 1)
	        return 1;
	    else
	        return n * factorial(n-1);
	}
}

     
	


